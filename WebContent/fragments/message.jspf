<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<%String message = (String)request.getSession().getAttribute(org.devichi.photo.control.servlet.PhotoConstants.SESSION_MESSAGE);
request.getSession().setAttribute(org.devichi.photo.control.servlet.PhotoConstants.SESSION_MESSAGE,null);
if (message != null && message.length() > 0) {%>
	<div class="alert">
		<c:out value="<%=message %>"/>
	</div>	
<%}%>

